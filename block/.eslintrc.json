{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:@wordpress/eslint-plugin/recommended", "prettier", "jsdoc"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "globals": {
    "__DEV__": "readonly",
    "Confetti": "readonly"
  },
  "plugins": ["react"],
  "rules": {
    "react/prop-types": "off"
  }
}
